# Server Configuration
PORT=3001

# LLM Provider Configuration
# Set to 'false' to disable Groq and use only Claude (default: true)
USE_GROQ=true

# Groq API Key (Llama 3.3 70B - Fast & Free Tier Available)
# Get your API key from: https://console.groq.com/
# FREE tier: 6,000 tokens/minute | 10-20x cheaper than Claude
# Used as primary LLM, falls back to Claude if unavailable
GROQ_API_KEY=your_groq_api_key_here

# Anthropic API Key (Claude 3.5 Sonnet - Fallback)
# Get your API key from: https://console.anthropic.com/
# Used as fallback when Groq is unavailable or disabled
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# AssemblyAI API Key (Transcript Extraction)
# Get your API key from: https://www.assemblyai.com/
ASSEMBLYAI_API_KEY=your_assemblyai_api_key_here
